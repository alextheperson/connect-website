{
  "compilerOptions": {
    "module": "system",
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "outFile": "../../built/local/tsc.js",
    "sourceMap": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true
  },
  "include": ["public/**/*", "src/**/*", "app.ts"],
  "exclude": ["**/*.spec.ts"]
}
